{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "mocha": true,
    "jasmine": true,
    "jest": true,
    "react-native/react-native": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "ecmaVersion": 6
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "@react-native-community"
  ],
  "plugins": [ "Jasmine", "react", "react-native" ],
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
