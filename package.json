{
  "name": "localball",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "build-debug": "detox build --configuration ios.sim.debug",
    "test:e2e": "detox test --configuration ios.sim.debug --loglevel info",
    "run-sim": "react-native run-ios --simulator \"iPhone X\"",
    "run-phone": "react-native run-ios --device \"Eric's iPhone\"",
    "lint": "./node_modules/.bin/eslint **/*.js"
  },
  "dependencies": {
    "@mapbox/react-native-mapbox-gl": "^6.1.3",
    "@slack/client": "^4.10.0",
    "firebase-admin": "^7.0.0",
    "geolib": "^2.0.24",
    "prop-types": "^15.6.2",
    "react": "16.6.1",
    "react-native": "^0.57.8",
    "react-native-elements": "1.0.0",
    "react-native-face-pile": "^1.9.0",
    "react-native-fbsdk": "^0.8.0",
    "react-native-firebase": "^5.1.1",
    "react-native-gesture-handler": "^1.0.9",
    "react-native-google-signin": "^1.0.1",
    "react-native-material-dropdown": "^0.11.1",
    "react-native-timeline-listview": "^0.2.3",
    "react-native-vector-icons": "^6.1.0",
    "react-navigation": "2.12.1",
    "react-redux": "^5.1.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "remote-redux-devtools": "^0.5.13"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-plugin-istanbul": "^5.1.1",
    "detox": "9.1.2",
    "eslint": "^5.12.0",
    "eslint-plugin-jasmine": "^2.10.1",
    "eslint-plugin-react": "^7.12.3",
    "jest": "^24.1.0",
    "metro-react-native-babel-preset": "^0.51.1",
    "react-test-renderer": "^16.8.3"
  },
  "rnpm": {
    "assets": [
      "./assets/"
    ]
  },
  "jest": {
    "preset": "react-native",
    "notify": true
  },
  "detox": {
    "test-runner": "jest",
    "runner-config": "jest/__e2e__/config.json",
    "specs": "e2e",
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/LocalBall.app",
        "build": "react-native run-ios",
        "type": "ios.simulator",
        "name": "iPhone X"
      }
    }
  },
  "resolutions": {
    "react-devtools-core": "3.4.3"
  }
}
